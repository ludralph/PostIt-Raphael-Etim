{
  "name": "postit",
  "version": "1.0.0",
  "engines": {
    "npm": "5.6.0",
    "node": "6.11.1"
  },
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --watch server --exec babel-node -- server/index.js",
    "test:server": "export NODE_ENV=test || SET \"NODE_ENV=test\" && nyc --reporter=html --reporter=text mocha --compilers js:babel-register server/test/**/*.test.js --timeout 15000 ",
    "test:client": "jest /client --coverage",
    "test": "npm run test:server",
    "build": "npm run remove-dist && npm run remove-build && npm run build:client && npm run build:server",
    "build:client": "webpack -p --config ./webpack.deployment.config.js --progress",
    "remove-dist": "node_modules/.bin/rimraf ./client/dist",
    "remove-build": "node_modules/.bin/rimraf ./build",
    "build:server": "babel -d ./build ./server -s --copy-files",
    "dev": "nodemon --watch server --exec babel-node -- server/index.js",
    "prod:start": "export NODE_ENV=production || SET \"NODE_ENV=production\" && npm start",
    "prod:build-start": "npm run build && npm run prod:start",
    "dev:watch": "nodemon --ignore webpack.config.js --exec \"npm run dev:start\"",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "lint:server": "eslint server",
    "lint:client": "eslint client",
    "lint": "npm run lint:server && npm run lint client",
    "webpack": "webpack",
    "heroku-postbuild": "npm run build",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test | coveralls",
    "coverage": "nyc  --reporter=text-lcov | coveralls",
    "e2e-setup": "selenium-standalone install",
    "e2e-server": "selenium-standalone start",
    "test:e2e": "nightwatch",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
